<?xml version="1.0" encoding="UTF-8" ?>

<routes xmlns="http://symfony.com/schema/routing"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/routing http://symfony.com/schema/routing/routing-1.0.xsd">

    <route id="shtumi_ajaxautocomplete" pattern="/shtumi_ajaxautocomplete">
        <default key="_controller">ShtumiUsefulBundle:AjaxAutocompleteJSON:getJSON</default>
    </route>

    <route id="shtumi_dependent_filtered_entity" pattern="/shtumi_dependent_filtered_entity">
        <default key="_controller">%shtumi_dependent_filtered_entity_controller_route%</default>
    </route>

</routes>